	// To store the keys in slice in sorted order
    var keys []int
    for k := range opcodeList {
        keys = append(keys, k)
    }
    sort.Ints(keys)

    // To perform the opertion you want
    for _, k := range keys {
        Println("Key:", k)
        Println("Value:", opcodeList[k])
    }
	return opcodeList
